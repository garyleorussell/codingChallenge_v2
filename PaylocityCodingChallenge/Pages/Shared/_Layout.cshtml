<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PaylocityCodingChallenge</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PaylocityCodingChallenge.styles.css" asp-append-version="true" />
    <script src="https://kit.fontawesome.com/e2d852ca78.js" crossorigin="anonymous"></script>
</head>
<body>
    <header class="container">
      <div class="row">
        <div class="col-2 text-center">
          <img class="img-fluid" src="images/logo.png" alt="paylocity">
        </div>
      </div>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - PaylocityCodingChallenge - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/benefitCostCalculator.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
 <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - PaylocityCodingChallenge - 
        </div>
    </footer>
</html>